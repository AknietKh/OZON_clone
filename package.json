{
  "name": "ozon_with_json-server",
  "version": "1.0.0",
  "description": "Небольшой клон интернет-магазина Ozon. Реализованы запросы на fake REST API для получения данных о товарах и добавления товаров в корзину.",
  "private": true,
  "homepage": "https://aknietkh.github.io/OZON_with_FakeRestApi-json-server-",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "dev": "cross-env NODE_ENV=development webpack --mode development",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "watch": "cross-env NODE_ENV=development webpack --mode development --watch",
    "sass": "sass ./src/scss/style.scss ./src/style/style.css",
    "json-server": "json-server --watch ./db/db.json",
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --open"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "gh-pages": "^2.2.0",
    "html-webpack-plugin": "^3.2.0",
    "json-server": "^0.15.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}
